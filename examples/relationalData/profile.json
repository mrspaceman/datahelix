{
  "fields": [
    { "name": "id", "type": "integer", "nullable": false },
    { "name": "firstname", "type": "faker.name.firstName", "nullable": false },
    { "name": "lastname", "type": "faker.name.lastName", "nullable": false },
    { "name": "registrationDate", "type": "datetime", "nullable": false },
    { "name": "lastOrderDate", "type": "integer", "nullable": false }
  ],
  "constraints": [
    { "field": "id", "greaterThan": 100 }
  ],
  "relationships": [ {
    "name": "orders",
    "cardinality": "OneToMany",
    "minItems": 0,
    "maxItems": 5,
    "profile": {
      "fields": [
        { "name": "id", "type": "integer", "nullable": false },
        { "name": "date", "type": "datetime", "nullable": false }
      ],
      "constraints": [
        { "field": "date", "afterOrAtField": "registrationDate", "relationship": "customer" }
      ]
    }
  } ]
}
